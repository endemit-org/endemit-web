            },
            "text_image_block": {
              "type": "SharedSlice"
            },
            "embed_block": {
              "type": "SharedSlice"
            },
            "newsletter_subscription": {
              "type": "SharedSlice"
            },
            "content_section": {
              "type": "SharedSlice"
            }
          }
        }
      }
