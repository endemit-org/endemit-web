{
  "id": "image_gallery",
  "type": "SharedSlice",
  "name": "ImageGallery",
  "description": "Image gallery with grid or masonry layout",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default",
      "imageUrl": "",
      "primary": {
        "heading": {
          "type": "StructuredText",
          "config": {
            "label": "Heading",
            "single": "heading2"
          }
        },
        "layout": {
          "type": "Select",
          "config": {
            "label": "Layout",
            "options": [
              "grid",
              "masonry"
            ],
            "default_value": "grid"
          }
        },
        "columns": {
          "type": "Select",
          "config": {
            "label": "Columns",
            "options": [
              "2",
              "3",
              "4"
            ],
            "default_value": "3"
          }
        },
        "include_frame": {
          "type": "Boolean",
          "config": {
            "label": "Include frame",
            "placeholder_false": "false",
            "placeholder_true": "true",
            "default_value": false
          }
        }
      },
      "items": {
        "image": {
          "type": "Image",
          "config": {
            "label": "Image"
          }
        },
        "caption": {
          "type": "Text",
          "config": {
            "label": "Caption"
          }
        }
      }
    }
  ]
}