{
  "name": "endemit-web",
  "version": "4.0.0",
  "private": true,
  "packageManager": "pnpm@9.13.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:inngest": "pnpm dlx inngest-cli@latest dev",
    "dev:emulated": "turbo dev dev:inngest",
    "build": "pnpm prisma:generate && pnpm prisma:deploy && pnpm build:next",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "slicemachine": "start-slicemachine",
    "postinstall": "pnpm prisma:generate",
    "env-init": "vercel login && vercel link --project=prj_vS6KCdGiPdIALZpEF5ZOlnGXperd --scope=team_PkvMkqbUl2Dicg1v7EZCAVue --yes && pnpm env-dev",
    "env-dev": "vercel env pull .env --environment=development",
    "env-stage": "vercel env pull .env --environment=preview",
    "env-prod": "vercel env pull .env --environment=production",
    "prisma:generate": "src/prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate": "src/prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma:deploy": "src/prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "prisma:push": "src/prisma db push --schema=./src/prisma/schema.prisma",
    "prisma:studio": "src/prisma studio --schema=./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@prismicio/client": "^7.20.1",
    "@prismicio/next": "^2.0.1",
    "@prismicio/react": "^3.2.1",
    "@react-email/components": "^0.5.6",
    "@react-email/render": "^1.3.2",
    "@react-email/tailwind": "^1.2.2",
    "@slicemachine/init": "^2.10.40",
    "@types/qrcode": "^1.5.5",
    "@types/sharp": "^0.32.0",
    "@vercel/analytics": "^1.5.0",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "crypto": "^1.0.1",
    "html-react-parser": "^5.2.7",
    "inngest": "^3.44.2",
    "nanoid": "^5.1.6",
    "next": "15.3.2",
    "prisma": "^6.17.1",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scroll-parallax": "^3.4.5",
    "resend": "^6.1.2",
    "sharp": "^0.34.2",
    "stripe": "^18.5.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@slicemachine/adapter-next": "^0.3.84",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "postcss": "^8.5.4",
    "prettier": "^3.6.2",
    "slice-machine-ui": "^2.18.2",
    "tailwindcss": "3.4.1",
    "turbo": "^2.5.8",
    "typescript": "^5"
  }
}
